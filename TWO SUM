class solution:
    def twosum(self,nums,target):
        for i in range(len(nums)-1):
            for j in range(i+1,len(nums)):
                if nums[i]+nums[j]== target:
                    return([i,j])
                
sol=solution()                
nums=[5,7,2,9]
target=14
result=sol.twosum(nums, target)
print("Indices of the two numbers ",result)
